<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="editDialog" title="Edit Entry" class="sapUiResponsivePadding--header sapUiResponsivePadding--content sapUiResponsivePadding--footer">
        <content>
            <VBox id="_IDGenVBox1">
                <Label id="_IDGenLabel1" text="Scheduling Agreement"/>
                <Input id="schedulingAgreementInput" value="{editData>/SchedulingAgreement}" placeholder="Scheduling Agreement"/>
                <Label id="_IDGenLabel2" text="Scheduling Agreement Item"/>
                <Input id="schedulingAgreementItemInput" value="{editData>/SchedulingAgreementItem}" placeholder="Scheduling Agreement Item"/>
                <Label id="_IDGenLabel3" text="Schedule Line"/>
                <Input id="scheduleLineInput" value="{editData>/ScheduleLine}" placeholder="Schedule Line"/>
                <Label id="_IDGenLabel4" text="Delivery Date Category"/>
                <Input id="delivDateCategoryInput" value="{editData>/DelivDateCategory}" placeholder="Delivery Date Category"/>
                <Label id="_IDGenLabel5" text="Delivery Date"/>
                <DatePicker id="scheduleLineDeliveryDateInput" value="{editData>/ScheduleLineDeliveryDate}" displayFormat="yyyy-MM-dd"/>
                <!-- Add more input fields for other properties as needed -->
            </VBox>
        </content>
        <beginButton>
            <Button id="_IDGenButton1" text="Save" press="onSavePress"/>
        </beginButton>
        <endButton>
            <Button id="_IDGenButton2" text="Cancel" press="onCancelPress"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>
